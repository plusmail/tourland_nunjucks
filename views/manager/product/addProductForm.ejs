                                <td>{%= tour.tname %}</td>
                                <td>{%= tour.tlocation %}</td>
                                <td>{%= tour.startDate %}</td>
                                <td>{%= tour.endDate %}</td>
                                <td>{%= tour.taddr %}</td>
                                <td>{%= tour.etime %}</td>
